# - Config file for the Garfield package
# It defines the following variables
#  GARFIELD_INCLUDE_DIRS - include directories for Garfield
#  GARFIELD_LIBRARIES - at the moment only Garfield
 
SET(GARFIELD_INCLUDE_DIRS
  "@CMAKE_INSTALL_PREFIX@/include/Garfiel"
  "@CMAKE_INSTALL_PREFIX@/include/Heed/"
  )
SET(GARFIELD_LIBRARIES Garfield)
  
SET( GARFIELD_SOURCE_DIR "@PROJECT_SOURCE_DIR@")
SET( GARFIELD_BINARY_DIR "@CMAKE_INSTALL_PREFIX@/bin")
SET( GARFIELD_LIBRARIES Garfield )
 
include("@CMAKE_INSTALL_PREFIX@/cmake/GarfieldLibraryDepends.cmake")
